{
  "name": "outl1ne/nova-multiselect-field",
  "description": "A multiple select field for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "multiselect",
    "select"
  ],
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.1",
    "nova-kit/nova-packages-tool": "^1.0.1",
    "outl1ne/nova-translations-loader": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Outl1ne\\MultiselectField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outl1ne\\MultiselectField\\FieldServiceProvider"
      ]
    }
  },
  "script": {
    "post-autoload-dump": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
